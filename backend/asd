VALUES (
    'Homemade Chocolate Chip Cookies',
    'Classic, chewy chocolate chip cookies with crisp edges and a soft center. The perfect treat!',
    'https://example.com/images/choc_chip_cookies.jpg',
    8,
    NOW(), 'Dessert', 'American', 24, 'medium', 'budget', 15, 0, 12, 350
);

DO $$
DECLARE
    cookies_id BIGINT;
BEGIN
    SELECT id INTO cookies_id FROM recipe WHERE title = 'Homemade Chocolate Chip Cookies'
    ORDER BY created_at DESC LIMIT 1;

    INSERT INTO ingredient (type, amount, unit, name, recipe_id)
    VALUES
    ('BUTTER', '1', 'CUP', 'Unsalted butter, softened', cookies_id),
    ('BROWN_SUGAR', '1', 'CUP', 'Brown sugar, packed', cookies_id),
    ('SUGAR', '1/2', 'CUP', 'Granulated sugar', cookies_id),
    ('EGGS', '2', 'PIECE', 'Large eggs', cookies_id),
    ('VANILLA_EXTRACT', '2', 'TEASPOON', 'Vanilla extract', cookies_id),
    ('FLOUR', '3', 'CUP', 'All-purpose flour', cookies_id),
    ('BAKING_SODA', '1', 'TEASPOON', 'Baking soda', cookies_id),
    ('SALT', '1', 'TEASPOON', 'Salt', cookies_id),
    ('CHOCOLATE_CHIPS', '2', 'CUP', 'Chocolate chips', cookies_id);

    INSERT INTO recipe_step (step_number, instruction, recipe_id)
    VALUES
    (0, 'Preheat oven to 350°F (175°C). Line baking sheets with parchment paper.', cookies_id),
    (1, 'In a large bowl, cream together the softened butter, brown sugar, and granulated sugar until light and fluffy.', cookies_id),
    (2, 'Beat in the eggs one at a time, then stir in the vanilla extract.', cookies_id),
    (3, 'In a separate bowl, whisk together the flour, baking soda, and salt.', cookies_id),
    (4, 'Gradually add the dry ingredients to the wet ingredients, mixing just until combined.', cookies_id),
    (5, 'Fold in the chocolate chips.', cookies_id),
    (6, 'Drop rounded tablespoons of dough onto the prepared baking sheets, spacing them about 2 inches apart.', cookies_id),
    (7, 'Bake for 10-12 minutes, or until the edges are lightly golden but the centers still look soft.', cookies_id),
    (8, 'Allow cookies to cool on the baking sheet for 5 minutes before transferring to a wire rack to cool completely.', cookies_id);
END $$;

