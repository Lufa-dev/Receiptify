INSERT INTO recipe (title, description, image_url, user_id, created_at, category, cuisine, servings, difficulty, cost_rating, prep_time, cook_time)
VALUES (
    'Classic Spaghetti Carbonara',
    'A traditional Roman pasta dish with eggs, cheese, pancetta, and black pepper.',
    'https://example.com/images/carbonara.jpg',
    6,
    NOW(), 'Main Course', 'Italian', 4, 'hard', 'moderate', 15, 20
);

DO $$
DECLARE
    carbonara_id BIGINT;
BEGIN
    SELECT id INTO carbonara_id FROM recipe WHERE title = 'Classic Spaghetti Carbonara'
    ORDER BY created_at DESC LIMIT 1;

    INSERT INTO ingredient (type, amount, unit, name, recipe_id)
    VALUES
    ('PASTA', '400', 'GRAM', 'Spaghetti', carbonara_id),
    ('EGGS', '4', 'PIECE', 'Large eggs', carbonara_id),
    ('PARMESAN_CHEESE', '100', 'GRAM', 'Parmesan cheese, grated', carbonara_id),
    ('BACON', '150', 'GRAM', 'Pancetta or guanciale, diced', carbonara_id),
    ('GARLIC', '2', 'PIECE', 'Garlic cloves, minced', carbonara_id),
    ('BLACK_PEPPER', '1', 'TEASPOON', 'Freshly ground black pepper', carbonara_id),
    ('SALT', '1', 'TEASPOON', 'Salt', carbonara_id);

    INSERT INTO recipe_step (step_number, instruction, recipe_id)
    VALUES
    (0, 'Bring a large pot of salted water to a boil and cook the spaghetti until al dente.', carbonara_id),
    (1, 'While the pasta is cooking, whisk together the eggs, grated Parmesan, and black pepper in a bowl.', carbonara_id),
    (2, 'In a large skillet, cook the pancetta over medium heat until crispy, about 5-7 minutes. Add the minced garlic and cook for 30 seconds until fragrant.', carbonara_id),
    (3, 'Drain the cooked pasta, reserving 1/2 cup of pasta water.', carbonara_id),
    (4, 'Working quickly, add the hot pasta to the skillet with the pancetta. Remove from heat.', carbonara_id),
    (5, 'Pour the egg mixture over the pasta and toss quickly to coat all strands. The residual heat will cook the eggs without scrambling them.', carbonara_id),
    (6, 'If needed, add a splash of reserved pasta water to create a creamy sauce.', carbonara_id),
    (7, 'Serve immediately with extra grated Parmesan and black pepper on top.', carbonara_id);
END $$;

