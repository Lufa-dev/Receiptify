    ('RICE', '2', 'CUP', 'Jasmine rice', thai_curry_id),
    ('VEGETABLE_BROTH', '1/2', 'CUP', 'Vegetable broth', thai_curry_id);

    INSERT INTO recipe_step (step_number, instruction, recipe_id)
    VALUES
    (0, 'Cook jasmine rice according to package instructions. Set aside.', thai_curry_id),
    (1, 'In a large skillet or wok, heat 2 tablespoons of oil over medium-high heat.', thai_curry_id),
    (2, 'Add garlic and ginger, stir-fry for 30 seconds until fragrant.', thai_curry_id),
    (3, 'Add the curry paste and cook for 1 minute, stirring constantly.', thai_curry_id),
    (4, 'Add the chicken or tofu and cook until the outside is no longer pink, about 3-4 minutes.', thai_curry_id),
    (5, 'Add the onion, bell pepper, and zucchini. Stir-fry for 2-3 minutes.', thai_curry_id),
    (6, 'Pour in the coconut milk and vegetable broth. Bring to a simmer.', thai_curry_id),
    (7, 'Add fish sauce, soy sauce, and brown sugar. Stir to combine.', thai_curry_id),
    (8, 'Simmer for 10-15 minutes until the vegetables are tender and the sauce has thickened slightly.', thai_curry_id),
    (9, 'Remove from heat and stir in lime juice and Thai basil leaves.', thai_curry_id),
    (10, 'Serve hot over jasmine rice.', thai_curry_id);
END $$;

INSERT INTO recipe (title, description, image_url, user_id, created_at, category, cuisine, servings, difficulty, cost_rating, prep_time, cook_time)
VALUES (
    'Classic French Onion Soup',
    'A rich, savory soup with caramelized onions and a cheesy toast topping. Pure comfort food!',
    'https://example.com/images/onion_soup.jpg',
    7,
    NOW(), 'Soup', 'French', 6, 'easy', 'budget', 15, 60
);

DO $$
DECLARE
    onion_soup_id BIGINT;
BEGIN
    SELECT id INTO onion_soup_id FROM recipe WHERE title = 'Classic French Onion Soup'
    ORDER BY created_at DESC LIMIT 1;

    INSERT INTO ingredient (type, amount, unit, name, recipe_id)
    VALUES
    ('ONIONS', '6', 'PIECE', 'Large yellow onions, thinly sliced', onion_soup_id),
    ('BUTTER', '4', 'TABLESPOON', 'Butter', onion_soup_id),
    ('OLIVE_OIL', '1', 'TABLESPOON', 'Olive oil', onion_soup_id),
    ('GARLIC', '2', 'PIECE', 'Garlic cloves, minced', onion_soup_id),
    ('FLOUR', '2', 'TABLESPOON', 'All-purpose flour', onion_soup_id),
    ('BEEF_BROTH', '8', 'CUP', 'Beef broth', onion_soup_id),
    ('RED_WINE', '1/2', 'CUP', 'Dry red wine', onion_soup_id),
    ('BAY_LEAVES', '2', 'PIECE', 'Bay leaves', onion_soup_id),
    ('THYME', '1', 'TEASPOON', 'Fresh thyme leaves', onion_soup_id),
    ('BREAD', '6', 'SLICE', 'French baguette slices', onion_soup_id),
    ('GRUYERE_CHEESE', '2', 'CUP', 'Gruyere cheese, grated', onion_soup_id),
    ('SALT', '1', 'TEASPOON', 'Salt', onion_soup_id),
    ('BLACK_PEPPER', '1/2', 'TEASPOON', 'Black pepper', onion_soup_id);

    INSERT INTO recipe_step (step_number, instruction, recipe_id)
    VALUES
    (0, 'In a large Dutch oven or heavy-bottomed pot, melt the butter with the olive oil over medium heat.', onion_soup_id),
    (1, 'Add the sliced onions and cook, stirring occasionally, for 45-50 minutes until deeply caramelized and golden brown. Be patient, this takes time but is essential for flavor.', onion_soup_id),
    (2, 'Add the minced garlic and cook for 1 minute until fragrant.', onion_soup_id),
    (3, 'Sprinkle the flour over the onions and stir to coat. Cook for 1-2 minutes.', onion_soup_id),
    (4, 'Add the red wine and deglaze the pot, scraping up any browned bits from the bottom.', onion_soup_id),
    (5, 'Add the beef broth, bay leaves, thyme, salt, and pepper. Bring to a simmer.', onion_soup_id),
    (6, 'Reduce heat to low, cover partially, and simmer for 30 minutes to develop flavors.', onion_soup_id),
    (7, 'Preheat the broiler. Place oven-safe soup bowls on a baking sheet.', onion_soup_id),
    (8, 'Remove bay leaves from the soup. Ladle the soup into the bowls.', onion_soup_id),
    (9, 'Place a slice of baguette on top of each bowl of soup, then sprinkle generously with grated Gruyere cheese.', onion_soup_id),
    (10, 'Broil for 2-3 minutes until the cheese is melted, bubbly, and golden brown.', onion_soup_id),
    (11, 'Serve immediately, being careful of the hot bowls.', onion_soup_id);
END $$;

INSERT INTO recipe (title, description, image_url, user_id, created_at, category, cuisine, servings, difficulty, cost_rating, prep_time, cook_time, baking_time, baking_temp)
VALUES (
    'Fresh Strawberry Cheesecake',
    'Creamy New York-style cheesecake with a buttery graham cracker crust, topped with fresh strawberry sauce.',
    'https://example.com/images/strawberry_cheesecake.jpg',
    3,
    NOW(), 'Dessert', 'American', 12, 'medium', 'moderate', 30, 0, 60, 325
);

DO $$
DECLARE
    cheesecake_id BIGINT;
BEGIN
    SELECT id INTO cheesecake_id FROM recipe WHERE title = 'Fresh Strawberry Cheesecake'
    ORDER BY created_at DESC LIMIT 1;

    INSERT INTO ingredient (type, amount, unit, name, recipe_id)
    VALUES
    ('CRACKERS', '2', 'CUP', 'Graham cracker crumbs', cheesecake_id),
    ('BUTTER', '1/2', 'CUP', 'Melted butter', cheesecake_id),
    ('SUGAR', '1/4', 'CUP', 'Granulated sugar', cheesecake_id),
    ('CREAM_CHEESE', '4', 'CUP', 'Cream cheese, softened (32 oz)', cheesecake_id),
    ('SUGAR', '1.5', 'CUP', 'Granulated sugar', cheesecake_id),
    ('VANILLA_EXTRACT', '2', 'TEASPOON', 'Vanilla extract', cheesecake_id),
    ('EGGS', '4', 'PIECE', 'Large eggs', cheesecake_id),
    ('SOUR_CREAM', '1', 'CUP', 'Sour cream', cheesecake_id),
    ('STRAWBERRIES', '2', 'CUP', 'Fresh strawberries, hulled and sliced', cheesecake_id),
    ('SUGAR', '1/4', 'CUP', 'Granulated sugar', cheesecake_id),
    ('CORNSTARCH', '1', 'TABLESPOON', 'Cornstarch', cheesecake_id),
    ('WATER', '2', 'TABLESPOON', 'Water', cheesecake_id),
    ('LEMON_JUICE', '1', 'TABLESPOON', 'Lemon juice', cheesecake_id);

    INSERT INTO recipe_step (step_number, instruction, recipe_id)
    VALUES
    (0, 'Preheat oven to 325°F (165°C). Grease a 9-inch springform pan and wrap the outside with aluminum foil to prevent water leakage.', cheesecake_id),
    (1, 'In a medium bowl, combine graham cracker crumbs, melted butter, and 1/4 cup sugar. Press mixture into the bottom of the prepared pan.', cheesecake_id),
    (2, 'Bake crust for 10 minutes. Remove from oven and let cool while preparing the filling.', cheesecake_id),
    (3, 'In a large mixing bowl, beat cream cheese and 1.5 cups sugar until smooth and creamy, about 2 minutes.', cheesecake_id),
    (4, 'Beat in vanilla extract. Add eggs one at a time, mixing on low speed just until each is incorporated.', cheesecake_id),
    (5, 'Fold in sour cream until just combined. Pour filling over the crust.', cheesecake_id),
    (6, 'Place the springform pan in a larger roasting pan. Fill the roasting pan with hot water to come halfway up the sides of the springform pan, creating a water bath.', cheesecake_id),
    (7, 'Bake for 55-60 minutes, or until the edges are set but the center still has a slight jiggle.', cheesecake_id),
    (8, 'Turn off the oven, crack the door open, and let the cheesecake cool in the oven for 1 hour.', cheesecake_id),
    (9, 'Remove from oven and water bath. Run a knife around the edge of the cheesecake to loosen it from the pan. Cool completely on a wire rack.', cheesecake_id),
    (10, 'Refrigerate for at least 4 hours or overnight.', cheesecake_id),
    (11, 'For the topping, combine 1 cup of strawberries with 1/4 cup sugar in a saucepan. Cook over medium heat until strawberries break down, about 5 minutes.', cheesecake_id),
    (12, 'Mix cornstarch with water to create a slurry. Add to the strawberry mixture and cook until thickened, about 1-2 minutes.', cheesecake_id),
    (13, 'Remove from heat and add lemon juice. Cool completely.', cheesecake_id),
    (14, 'Just before serving, fold the remaining fresh strawberries into the cooled sauce and spread over the cheesecake.', cheesecake_id);
END $$;

INSERT INTO recipe (title, description, image_url, user_id, created_at, category, cuisine, servings, difficulty, cost_rating, prep_time, cook_time, baking_time, baking_temp)
VALUES (
    'Homemade Margherita Pizza',
    'Classic Italian pizza with a thin, crispy crust topped with fresh tomatoes, mozzarella, and basil.',
    'https://example.com/images/margherita_pizza.jpg',
    12,
    NOW(), 'Main Course', 'Italian', 4, 'medium', 'budget', 30, 0, 15, 475
);

DO $
DECLARE
    pizza_id BIGINT;
BEGIN
    SELECT id INTO pizza_id FROM recipe WHERE title = 'Homemade Margherita Pizza'
    ORDER BY created_at DESC LIMIT 1;

    INSERT INTO ingredient (type, amount, unit, name, recipe_id)
    VALUES
    ('FLOUR', '3.5', 'CUP', 'Bread flour or all-purpose flour', pizza_id),
    ('YEAST', '1', 'TEASPOON', 'Active dry yeast', pizza_id),
    ('SUGAR', '1', 'TEASPOON', 'Granulated sugar', pizza_id),
    ('SALT', '1.5', 'TEASPOON', 'Salt', pizza_id),
    ('OLIVE_OIL', '2', 'TABLESPOON', 'Extra virgin olive oil', pizza_id),
    ('WATER', '1.5', 'CUP', 'Warm water (105-110°F)', pizza_id),
    ('TOMATOES', '3', 'PIECE', 'Fresh Roma tomatoes, thinly sliced', pizza_id),
    ('MOZZARELLA', '8', 'OUNCE', 'Fresh mozzarella cheese, torn into pieces', pizza_id),
    ('BASIL', '1/4', 'CUP', 'Fresh basil leaves', pizza_id),
    ('OLIVE_OIL', '2', 'TABLESPOON', 'Extra virgin olive oil for drizzling', pizza_id),
    ('GARLIC', '2', 'PIECE', 'Garlic cloves, minced', pizza_id),
    ('SALT', '1/2', 'TEASPOON', 'Sea salt', pizza_id),
    ('BLACK_PEPPER', '1/4', 'TEASPOON', 'Freshly ground black pepper', pizza_id);

    INSERT INTO recipe_step (step_number, instruction, recipe_id)
    VALUES
    (0, 'In a large bowl, mix flour, salt, and sugar. In a separate small bowl, combine warm water and yeast. Let it sit for 5 minutes until foamy.', pizza_id),
    (1, 'Add the yeast mixture and olive oil to the flour mixture. Mix until a dough forms.', pizza_id),
    (2, 'Turn dough out onto a floured surface and knead for about 5-7 minutes until smooth and elastic.', pizza_id),
    (3, 'Place dough in an oiled bowl, cover with a damp cloth, and let rise in a warm place for 1-2 hours, or until doubled in size.', pizza_id),
    (4, 'Preheat oven to 475°F (245°C). If you have a pizza stone, place it in the oven while preheating.', pizza_id),
    (5, 'Punch down the dough and divide it into 2 equal portions. (You can freeze one portion for later use if desired).', pizza_id),
    (6, 'On a floured surface, roll or stretch the dough into a 12-inch circle.', pizza_id),
    (7, 'Transfer the dough to a pizza peel or baking sheet lined with parchment paper.', pizza_id),
    (8, 'Brush the dough with olive oil and sprinkle with minced garlic.', pizza_id),
    (9, 'Arrange tomato slices and mozzarella pieces evenly over the dough.', pizza_id),
    (10, 'Sprinkle with salt and pepper.', pizza_id),
    (11, 'Bake for 12-15 minutes, or until the crust is golden brown and the cheese is bubbly.', pizza_id),
    (12, 'Remove from oven and immediately top with fresh basil leaves and a drizzle of olive oil.', pizza_id),
    (13, 'Slice and serve hot.', pizza_id);
END $;

