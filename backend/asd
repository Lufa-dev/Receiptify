INSERT INTO recipe (title, description, image_url, user_id, created_at, category, cuisine, servings, difficulty, cost_rating, prep_time, cook_time)
VALUES (
    'Classic Beef Stroganoff',
    'Tender strips of beef in a rich, creamy mushroom sauce served over egg noodles.',
    'https://example.com/images/stroganoff.jpg',
    4,
    NOW(), 'Main Course', 'Russian', 4, 'medium', 'moderate', 20, 30
);

DO $$
DECLARE
    stroganoff_id BIGINT;
BEGIN
    SELECT id INTO stroganoff_id FROM recipe WHERE title = 'Classic Beef Stroganoff'
    ORDER BY created_at DESC LIMIT 1;

    INSERT INTO ingredient (type, amount, unit, name, recipe_id)
    VALUES
    ('BEEF_SIRLOIN', '1', 'POUND', 'Beef sirloin, thinly sliced', stroganoff_id),
    ('MUSHROOMS', '8', 'OUNCE', 'Button mushrooms, sliced', stroganoff_id),
    ('ONIONS', '1', 'PIECE', 'Large onion, diced', stroganoff_id),
    ('GARLIC', '2', 'PIECE', 'Garlic cloves, minced', stroganoff_id),
    ('BUTTER', '3', 'TABLESPOON', 'Butter', stroganoff_id),
    ('FLOUR', '2', 'TABLESPOON', 'All-purpose flour', stroganoff_id),
    ('BEEF_BROTH', '1.5', 'CUP', 'Beef broth', stroganoff_id),
    ('SOUR_CREAM', '1', 'CUP', 'Sour cream', stroganoff_id),
    ('DIJON_MUSTARD', '1', 'TABLESPOON', 'Dijon mustard', stroganoff_id),
    ('NOODLES', '8', 'OUNCE', 'Egg noodles', stroganoff_id),
    ('PAPRIKA', '1', 'TEASPOON', 'Paprika', stroganoff_id),
    ('SALT', '1', 'TEASPOON', 'Salt', stroganoff_id),
    ('BLACK_PEPPER', '1/2', 'TEASPOON', 'Black pepper', stroganoff_id);

    INSERT INTO recipe_step (step_number, instruction, recipe_id)
    VALUES
    (0, 'Bring a large pot of salted water to a boil and cook egg noodles according to package directions. Drain and set aside.', stroganoff_id),
    (1, 'Season beef slices with salt, pepper, and paprika.', stroganoff_id),
    (2, 'In a large skillet, melt 1 tablespoon of butter over high heat. Quickly sear the beef on both sides until browned but still rare inside, about 1 minute per side. Remove from skillet and set aside.', stroganoff_id),
    (3, 'In the same skillet, melt the remaining butter. Add the onions and cook until softened, about 3 minutes. Add the mushrooms and cook until they release their moisture and begin to brown, about 5-7 minutes.', stroganoff_id),
    (4, 'Add garlic and cook for 30 seconds until fragrant.', stroganoff_id),
    (5, 'Sprinkle flour over the vegetables and stir to coat. Cook for 1 minute.', stroganoff_id),
    (6, 'Gradually whisk in the beef broth, scraping up any browned bits from the bottom of the pan. Bring to a simmer and cook until the sauce thickens, about 5 minutes.', stroganoff_id),
    (7, 'Reduce heat to low. Stir in the Dijon mustard, then the sour cream, stirring continuously until combined.', stroganoff_id),
    (8, 'Return the beef to the skillet along with any accumulated juices. Simmer gently for 1-2 minutes to warm the meat through.', stroganoff_id),
    (9, 'Serve over cooked egg noodles, garnished with fresh parsley if desired.', stroganoff_id);
END $$;

